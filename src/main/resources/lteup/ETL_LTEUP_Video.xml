				<field id="referer" />
				<field id="ts_pl_dl_n" />
				<field id="cookie" />
				<field id="ttl" />
			</fields>
		</sourceMetaData>
		<outputMetaData outputPath="${outputPath}" errorPath="${errorPath}">
			<values delimiter="|">
				<field id="IMSI" />
				<field id="MSISDN" />
				<field id="pt_tsn" />
				<field id="Tid" />
				<field id="CGI" />
				<field id="TAC" />
				<field id="network" />
				<field id="eNB_id" />
				<field id="eNB_ip" />
				<field id="SGW_ip" />
