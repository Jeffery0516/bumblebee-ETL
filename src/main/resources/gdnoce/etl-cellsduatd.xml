<?xml version = "1.0" encoding = "utf-8"?>
<etl>
	<contextProperty location="etl-cellsduatd.properties" />
	
	<process id="cellsduatd">
		<computingFrameworkConfigs>
			<property key="outputFormatClass"
				value="com.gsta.bigdata.etl.mapreduce.OnlyValueOutputFormat"/>
			<property key="mapred.reduce.tasks" value="0"/>
		</computingFrameworkConfigs>
		<sourceMetaData type="zteENODEBXML" >
			<paths>
				<inputPath path="${inputPath}"/>
			</paths>
		</sourceMetaData>
		<outputMetaData outputPath="${outputPath}" errorPath="${outputPath}">
			<values delimiter="|">
				<field id="COLLECTTIME" />
				<field id="SBNID" />
				<field id="ENODEBID" />
				<field id="CellID" />
				<field id="C373364100" />
				<field id="C373364101" />
				<field id="C373364102" />
				<field id="C373364103" />
				<field id="C373364104" />
				<field id="C373364105" />
				<field id="C373364106" />
				<field id="C373364107" />
				<field id="C373364108" />
			</values>
		</outputMetaData>
	</process>
</etl>